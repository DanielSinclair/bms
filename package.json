    "browser-market",
    "webstore",
    "web-ext",
    "addons",
    "submission",
    "browser-addons",
    "firefox",
    "chrome",
    "edge"
  ],
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "29.0.3",
    "@plasmohq/prettier-plugin-sort-imports": "3.5.1",
    "@plasmohq/rps": "1.5.1",
    "@types/node": "18.7.20",
    "cross-env": "7.0.3",
    "esbuild": "0.15.9",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
